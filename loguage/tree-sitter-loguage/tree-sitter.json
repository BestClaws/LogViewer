{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "loguage",
      "camelcase": "Loguage",
      "title": "Loguage",
      "scope": "source.loguage",
      "file-types": [
        "lge"
      ],
      "injection-regex": "^loguage$",
      "class-name": "TreeSitterLoguage"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "LogViewer language parser",
    "authors": [
      {
        "name": "Syed Liyakhath"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-loguage"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}